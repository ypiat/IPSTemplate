{
  "DefaultApiVersion": "1.0",
  "AppSettings": {
    "BaseAddress": "{BaseAddress}",
    "HttpClientTimeout": "300"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "HangFire": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "%AppData%/Logs/IPSTemplate.AppServer.log",
          "rollingInterval": "Day",
          "buffered": false
        }
      },
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "ContextConnectionString": "{ContextConnectionString}"
  }
}
